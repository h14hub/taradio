{
  "name": "taradio_backends",
  "version": "1.0.0",
  "description": "",
  "repository": "",
  "author": "",
  "license": "",
  "main": "dist/index.js",
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "serve_front": "cd frontend && vue-cli-service serve",
    "build_front": "cd frontend && npx vue-cli-service build --mode production && rm -rf ../dist_front && mv dist ../dist_front",
    "lint-front": "cd frontend && vue-cli-service lint",
    "heroku-prebuild": "npm run build_front",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "aws-sdk": "^2.578.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "google-geocoder": "^0.2.1",
    "mongoose": "^5.7.11",
    "morgan": "^1.9.1",
    "pug": "^2.0.0-beta11",
    "rimraf": "^2.6.2",
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "dotenv": "^8.2.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-async-computed": "^3.7.0",
    "vue-form-generator": "^2.3.4",
    "vue-router": "^3.1.3",
    "vue-material": "^1.0.0-beta-11",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-register": "^6.26.0",
    "dotenv": "^8.2.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^3.1.3",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-pwa": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
